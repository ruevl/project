DEBUG=true

# Database
DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/library_db

# JWT (–û–ë–Ø–ó–ê–¢–ï–õ–¨–ù–û –∏–∑–º–µ–Ω–∏—Ç—å!)
