REDIS_PORT=6379
CACHE_TTL=300

# CORS (–¥–ª—è production —É–∫–∞–∑–∞—Ç—å –∫–æ–Ω–∫—Ä–µ—Ç–Ω—ã–µ origins)
CORS_ORIGINS=["http://localhost:3000"]

# OpenLibrary
